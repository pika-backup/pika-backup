<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.10" />
  <requires lib="libadwaita" version="1.4" />
  <object class="GtkImage" id="detail_repo_icon">
    <style>
      <class name="large-row-icon" />
    </style>
  </object>
  <menu id="app_menu">
    <section>
      <item>
        <attribute name="label" translatable="yes">_Keyboard Shortcuts</attribute>
        <attribute name="action">win.show-help-overlay</attribute>
      </item>
      <item>
        <attribute name="label" translatable="yes">_Help</attribute>
        <attribute name="action">app.help</attribute>
      </item>
      <item>
        <attribute name="label" translatable="yes">_About Pika Backup</attribute>
        <attribute name="action">app.about</attribute>
      </item>
    </section>
  </menu>
  <menu id="detail_menu">
    <section>
      <item>
        <attribute name="label" translatable="yes">Backup Preferences</attribute>
        <attribute name="action">app.backup-preferences</attribute>
      </item>
      <item>
        <attribute name="label" translatable="yes">Remove Backup Setup</attribute>
        <attribute name="action">app.remove</attribute>
      </item>
    </section>
  </menu>
  <object class="GtkPopover" id="schedule_preferred_time_popover">
    <property name="child">
      <object class="GtkBox">
        <property name="margin-start">18</property>
        <property name="margin-end">18</property>
        <property name="margin-top">18</property>
        <property name="margin-bottom">18</property>
        <property name="spacing">6</property>
        <child>
          <object class="GtkSpinButton" id="schedule_preferred_hour">
            <property name="max-width-chars">2</property>
            <property name="orientation">vertical</property>
            <property name="adjustment">
              <object class="GtkAdjustment">
                <property name="upper">23</property>
                <property name="step-increment">1</property>
                <property name="page-increment">10</property>
              </object>
            </property>
            <property name="numeric">1</property>
          </object>
        </child>
        <child>
          <object class="GtkLabel">
            <property name="label" translatable="yes">:</property>
          </object>
        </child>
        <child>
          <object class="GtkSpinButton" id="schedule_preferred_minute">
            <property name="max-width-chars">2</property>
            <property name="orientation">vertical</property>
            <property name="adjustment">
              <object class="GtkAdjustment">
                <property name="upper">59</property>
                <property name="step-increment">1</property>
                <property name="page-increment">10</property>
              </object>
            </property>
            <property name="numeric">1</property>
          </object>
        </child>
      </object>
    </property>
  </object>
  <object class="AdwApplicationWindow" id="window">
    <property name="width-request">360</property>
    <property name="height-request">294</property>
    <property name="default-width">782</property>
    <property name="default-height">626</property>
    <property name="title" translatable="yes">Pika Backup</property>
    <child>
      <object class="AdwBreakpoint">
        <condition>max-width: 480sp</condition>
        <setter object="detail_header_bar" property="title-widget" />
        <setter object="detail_view_switcher_bar" property="reveal">1</setter>
      </object>
    </child>
    <property name="content">
      <object class="AdwToastOverlay" id="toast">
        <property name="child">
          <object class="AdwNavigationView" id="navigation_view">
            <child>
              <object class="AdwNavigationPage" id="navigation_page_overview">
                <property name="title" translatable="yes">Pika Backup</property>
                <property name="child">
                  <object class="AdwToolbarView" id="overview">
                    <child type="top">
                      <object class="AdwHeaderBar">
                        <child type="start">
                          <object class="GtkButton" id="add_backup">
                            <property name="tooltip-text" translatable="yes">Setup Backup</property>
                            <property name="icon-name">list-add-symbolic</property>
                          </object>
                        </child>
                        <child type="end">
                          <object class="GtkMenuButton" id="primary_menu_button">
                            <property name="menu-model">app_menu</property>
                            <property name="tooltip-text" translatable="yes">Main Menu</property>
                            <property name="icon-name">open-menu-symbolic</property>
                            <property name="primary">true</property>
                          </object>
                        </child>
                      </object>
                    </child>
                    <property name="content">
                      <object class="GtkBox">
                        <property name="hexpand">1</property>
                        <property name="vexpand">1</property>
                        <property name="orientation">vertical</property>
                        <child>
                          <object class="AdwViewStack" id="main_stack">
                            <property name="vexpand">1</property>
                            <child>
                              <object class="AdwViewStackPage">
                                <property name="child">
                                  <object class="AdwStatusPage" id="page_overview_empty">
                                    <property name="icon-name">overview-symbolic</property>
                                    <property name="title" translatable="yes">No Backup Configured</property>
                                    <property name="description" translatable="yes">Add a new configuration to start creating backups</property>
                                    <property name="child">
                                      <object class="GtkButton" id="add_backup_empty">
                                        <property name="label" translatable="yes">_Setup Backup</property>
                                        <property name="use-underline">true</property>
                                        <property name="halign">center</property>
                                        <style>
                                          <class name="pill" />
                                          <class name="suggested-action" />
                                        </style>
                                      </object>
                                    </property>
                                  </object>
                                </property>
                              </object>
                            </child>
                            <child>
                              <object class="AdwViewStackPage">
                                <property name="child">
                                  <object class="AdwPreferencesPage" id="page_overview">
                                    <child>
                                      <object class="AdwPreferencesGroup">
                                        <child>
                                          <object class="GtkListBox" id="main_backups">
                                            <property name="valign">start</property>
                                            <property name="selection-mode">none</property>
                                            <style>
                                              <class name="overview-list" />
                                            </style>
                                          </object>
                                        </child>
                                      </object>
                                    </child>
                                  </object>
                                </property>
                              </object>
                            </child>
                          </object>
                        </child>
                      </object>
                    </property>
                  </object>
                </property>
              </object>
            </child>
            <child>
              <object class="AdwNavigationPage" id="navigation_page_detail">
                <property name="title" translatable="yes">Backup</property>
                <property name="child">
                  <object class="AdwToolbarView" id="page_detail">
                    <child type="top">
                      <object class="AdwHeaderBar" id="detail_header_bar">
                        <property name="title-widget">
                          <object class="AdwViewSwitcher" id="detail_view_switcher">
                            <property name="policy">wide</property>
                            <property name="stack">detail_stack</property>
                          </object>
                        </property>
                        <child type="end">
                          <object class="GtkMenuButton" id="pending_menu">
                            <property name="visible">0</property>
                            <property name="popover">
                              <object class="GtkPopover">
                                <property name="child">
                                  <object class="GtkLabel">
                                    <property name="margin-start">12</property>
                                    <property name="margin-end">12</property>
                                    <property name="margin-top">12</property>
                                    <property name="margin-bottom">12</property>
                                    <property name="label" translatable="yes">Archives are being mounted. This might take a while.</property>
                                  </object>
                                </property>
                              </object>
                            </property>
                            <child>
                              <object class="GtkSpinner" id="pending_menu_spinner" />
                            </child>
                          </object>
                        </child>
                        <child type="end">
                          <object class="GtkMenuButton">
                            <property name="menu-model">detail_menu</property>
                            <property name="tooltip-text" translatable="yes">Actions</property>
                            <property name="icon-name">view-more-symbolic</property>
                            <child>
                              <object class="GtkShortcutController">
                                <property name="scope">managed</property>
                                <child>
                                  <object class="GtkShortcut">
                                    <property name="trigger">F10</property>
                                    <property name="action">activate</property>
                                  </object>
                                </child>
                              </object>
                            </child>
                          </object>
                        </child>
                      </object>
                    </child>
                    <property name="content">
                      <object class="GtkBox">
                        <property name="orientation">vertical</property>
                        <child>
                          <object class="AdwViewStack" id="detail_stack">
                            <property name="vexpand">1</property>
                            <child>
                              <object class="AdwViewStackPage">
                                <property name="use-underline">1</property>
                                <property name="title" translatable="yes">_Backup</property>
                                <property name="icon-name">drive-harddisk-symbolic</property>
                                <property name="child">
                                  <object class="AdwPreferencesPage" id="page_backup">
                                    <child>
                                      <object class="AdwPreferencesGroup">
                                        <child>
                                          <object class="AdwActionRow" id="detail_repo_row">
                                            <property name="activatable">True</property>
                                            <child type="suffix">
                                              <object class="GtkButton" id="backup_disk_eject_button">
                                                <property name="tooltip-text" translatable="yes">Eject Backup Disk</property>
                                                <property name="icon-name">media-eject-symbolic</property>
                                                <property name="valign">center</property>
                                                <style>
                                                  <class name="flat" />
                                                </style>
                                              </object>
                                            </child>
                                            <child type="suffix">
                                              <object class="GtkBox" id="backup_disk_disconnected">
                                                <property name="valign">center</property>
                                                <style>
                                                  <class name="tag-box" />
                                                </style>
                                                <child>
                                                  <object class="GtkLabel">
                                                    <property name="label" translatable="yes">Disconnected</property>
                                                    <style>
                                                      <class name="tag" />
                                                    </style>
                                                  </object>
                                                </child>
                                              </object>
                                            </child>
                                          </object>
                                        </child>
                                        <child>
                                          <object class="PikaStatusRow" id="detail_status_row">
                                            <property name="activatable">1</property>
                                            <child type="suffix">
                                              <object class="GtkBox">
                                                <child>
                                                  <object class="GtkButton" id="stop_backup_create">
                                                    <property name="visible">0</property>
                                                    <property name="label" translatable="yes">_Abort</property>
                                                    <property name="halign">center</property>
                                                    <property name="valign">center</property>
                                                    <property name="use-underline">1</property>
                                                  </object>
                                                </child>
                                                <child>
                                                  <object class="GtkImage" id="detail_hint_icon">
                                                    <property name="visible">0</property>
                                                    <property name="icon-name">go-next-symbolic</property>
                                                    <style>
                                                      <class name="dim-label" />
                                                    </style>
                                                  </object>
                                                </child>
                                              </object>
                                            </child>
                                          </object>
                                        </child>
                                        <child>
                                          <object class="GtkButton" id="backup_run">
                                            <property name="use-underline">1</property>
                                            <property name="label" translatable="yes">Back Up _Now</property>
                                            <property name="halign">center</property>
                                            <property name="valign">start</property>
                                            <property name="margin-top">20</property>
                                            <property name="width-request">200</property>
                                            <style>
                                              <class name="suggested-action" />
                                              <class name="pill" />
                                            </style>
                                          </object>
                                        </child>
                                      </object>
                                    </child>
                                    <child>
                                      <object class="AdwPreferencesGroup">
                                        <property name="title" translatable="yes">Files to Back Up</property>
                                        <property name="description" translatable="yes">Only files in the following folders are saved in the backup repository.</property>
                                        <property name="header-suffix">
                                          <object class="GtkButton" id="add_include">
                                            <property name="tooltip-text" translatable="yes">Select Directory to Include</property>
                                            <property name="icon-name">list-add-symbolic</property>
                                            <property name="valign">start</property>
                                          </object>
                                        </property>
                                        <child>
                                          <object class="GtkListBox" id="include">
                                            <property name="selection-mode">none</property>
                                            <style>
                                              <class name="boxed-list" />
                                            </style>
                                            <child type="placeholder">
                                              <object class="AdwActionRow">
                                                <property name="title" translatable="yes">No files included in backup</property>
                                                <style>
                                                  <class name="error" />
                                                </style>
                                              </object>
                                            </child>
                                          </object>
                                        </child>
                                      </object>
                                    </child>
                                    <child>
                                      <object class="AdwPreferencesGroup">
                                        <property name="title" translatable="yes">Exclude from Backup</property>
                                        <property name="description" translatable="yes">The following folders are excluded from the backup repository.</property>
                                        <property name="header-suffix">
                                          <object class="GtkButton" id="add_exclude">
                                            <property name="tooltip-text" translatable="yes">Select Directory to Exclude</property>
                                            <property name="icon-name">list-add-symbolic</property>
                                            <property name="valign">start</property>
                                          </object>
                                        </property>
                                        <child>
                                          <object class="GtkListBox" id="backup_exclude">
                                            <property name="selection-mode">none</property>
                                            <style>
                                              <class name="boxed-list" />
                                            </style>
                                            <child type="placeholder">
                                              <object class="AdwActionRow">
                                                <property name="title" translatable="yes">Nothing excluded from backup</property>
                                              </object>
                                            </child>
                                          </object>
                                        </child>
                                      </object>
                                    </child>
                                  </object>
                                </property>
                              </object>
                            </child>
                            <child>
                              <object class="AdwViewStackPage">
                                <property name="use-underline">1</property>
                                <property name="title" translatable="yes">_Archives</property>
                                <property name="icon-name">drawer-symbolic</property>
                                <property name="child">
                                  <object class="AdwPreferencesPage" id="page_archives">
                                    <child>
                                      <object class="AdwPreferencesGroup">
                                        <child>
                                          <object class="AdwPreferencesRow">
                                            <property name="activatable">0</property>
                                            <child>
                                              <object class="GtkBox">
                                                <property name="spacing">6</property>
                                                <property name="margin-start">12</property>
                                                <property name="margin-end">12</property>
                                                <property name="margin-top">6</property>
                                                <property name="margin-bottom">6</property>
                                                <child>
                                                  <object class="GtkImage" id="archives_location_icon">
                                                    <property name="icon-name">folder-symbolic</property>
                                                    <property name="valign">start</property>
                                                    <property name="pixel-size">64</property>
                                                  </object>
                                                </child>
                                                <child>
                                                  <object class="GtkBox">
                                                    <property name="orientation">vertical</property>
                                                    <property name="hexpand">1</property>
                                                    <property name="spacing">3</property>
                                                    <style>
                                                      <class name="overview-location-box" />
                                                    </style>
                                                    <child>
                                                      <object class="GtkBox">
                                                        <child>
                                                          <object class="GtkBox">
                                                            <property name="orientation">vertical</property>
                                                            <child>
                                                              <object class="GtkLabel" id="archives_location_title">
                                                                <property name="xalign">0</property>
                                                                <property name="wrap">1</property>
                                                                <style>
                                                                  <class name="heading" />
                                                                </style>
                                                              </object>
                                                            </child>
                                                            <child>
                                                              <object class="GtkLabel" id="archives_location_subtitle">
                                                                <property name="xalign">0</property>
                                                                <property name="wrap">1</property>
                                                                <style>
                                                                  <class name="subtitle" />
                                                                </style>
                                                              </object>
                                                            </child>
                                                          </object>
                                                        </child>
                                                        <child>
                                                          <object class="GtkBox">
                                                            <property name="orientation">vertical</property>
                                                            <child>
                                                              <object class="GtkLabel" id="archives_location_suffix_title">
                                                                <property name="xalign">1</property>
                                                                <property name="hexpand">1</property>
                                                                <property name="wrap">1</property>
                                                                <property name="label">700 MB</property>
                                                              </object>
                                                            </child>
                                                            <child>
                                                              <object class="GtkLabel" id="archives_location_suffix_subtitle">
                                                                <property name="xalign">1</property>
                                                                <property name="wrap">1</property>
                                                                <property name="label">1 GB</property>
                                                                <style>
                                                                  <class name="subtitle" />
                                                                </style>
                                                              </object>
                                                            </child>
                                                          </object>
                                                        </child>
                                                      </object>
                                                    </child>
                                                    <child>
                                                      <object class="GtkLevelBar" id="archives_fs_usage">
                                                        <property name="margin-top">9</property>
                                                      </object>
                                                    </child>
                                                  </object>
                                                </child>
                                              </object>
                                            </child>
                                          </object>
                                        </child>
                                        <child>
                                          <object class="AdwActionRow">
                                            <property name="activatable-widget">archives_prefix_edit</property>
                                            <property name="title" translatable="yes">Archive _Prefix</property>
                                            <property name="use-underline">1</property>
                                            <property name="subtitle" translatable="yes">Added in front of all archive names</property>
                                            <property name="icon-name">emblem-documents-symbolic</property>
                                            <child type="suffix">
                                              <object class="GtkBox">
                                                <property name="spacing">6</property>
                                                <child>
                                                  <object class="GtkLabel" id="archives_prefix">
                                                    <style>
                                                      <class name="dim-label" />
                                                    </style>
                                                  </object>
                                                </child>
                                                <child>
                                                  <object class="GtkButton" id="archives_prefix_edit">
                                                    <property name="icon-name">document-edit-symbolic</property>
                                                    <property name="valign">center</property>
                                                    <style>
                                                      <class name="flat" />
                                                    </style>
                                                  </object>
                                                </child>
                                              </object>
                                            </child>
                                          </object>
                                        </child>
                                        <child>
                                          <object class="AdwActionRow" id="archives_cleanup">
                                            <property name="activatable">1</property>
                                            <property name="use-underline">1</property>
                                            <property name="title" translatable="yes">_Cleanup Archives</property>
                                            <property name="subtitle" translatable="yes">Delete some older archives</property>
                                            <property name="icon-name">user-trash-symbolic</property>
                                            <child type="suffix">
                                              <object class="GtkImage">
                                                <property name="icon-name">go-next-symbolic</property>
                                                <style>
                                                  <class name="dim-label" />
                                                </style>
                                              </object>
                                            </child>
                                          </object>
                                        </child>
                                      </object>
                                    </child>
                                    <child>
                                      <object class="AdwPreferencesGroup">
                                        <property name="title" translatable="yes">Data Integrity Check</property>
                                        <child>
                                          <object class="PikaStatusRow" id="check_status">
                                            <property name="activatable">1</property>
                                            <child type="suffix">
                                              <object class="GtkButton" id="archives_check_now">
                                                <property name="label" translatable="yes">_Check Now…</property>
                                                <property name="use-underline">1</property>
                                                <property name="valign">center</property>
                                              </object>
                                            </child>
                                            <child type="suffix">
                                              <object class="GtkButton" id="archives_check_abort">
                                                <property name="visible">0</property>
                                                <property name="label" translatable="yes">_Abort…</property>
                                                <property name="use-underline">1</property>
                                                <property name="valign">center</property>
                                              </object>
                                            </child>
                                          </object>
                                        </child>
                                      </object>
                                    </child>
                                    <child>
                                      <object class="AdwPreferencesGroup">
                                        <property name="title" translatable="yes">Archives</property>
                                        <property name="header-suffix">
                                          <object class="GtkBox">
                                            <property name="spacing">6</property>
                                            <child>
                                              <object class="GtkStack" id="archives_reloading_stack">
                                                <property name="transition-duration">0</property>
                                                <child>
                                                  <object class="GtkButton" id="refresh_archives">
                                                    <property name="tooltip-text" translatable="yes">Refresh</property>
                                                    <property name="icon-name">view-refresh-symbolic</property>
                                                    <style>
                                                      <class name="flat" />
                                                    </style>
                                                  </object>
                                                </child>
                                                <child>
                                                  <object class="GtkStackPage">
                                                    <property name="child">
                                                      <object class="GtkSpinner" id="archives_reloading_spinner">
                                                        <property name="halign">center</property>
                                                        <property name="valign">center</property>
                                                      </object>
                                                    </property>
                                                  </object>
                                                </child>
                                              </object>
                                            </child>
                                            <child>
                                              <object class="GtkButton" id="archives_eject_button">
                                                <property name="tooltip-text" translatable="yes">Stop browsing archive files</property>
                                                <property name="icon-name">media-eject-symbolic</property>
                                                <style>
                                                  <class name="flat" />
                                                </style>
                                              </object>
                                            </child>
                                          </object>
                                        </property>
                                        <child>
                                          <object class="GtkStack" id="archives_stack">
                                            <property name="hhomogeneous">0</property>
                                            <property name="vhomogeneous">0</property>
                                            <child>
                                              <object class="GtkListBox" id="archive_list">
                                                <property name="selection-mode">none</property>
                                                <style>
                                                  <class name="boxed-list" />
                                                </style>
                                              </object>
                                            </child>
                                            <child>
                                              <object class="GtkListBox" id="archive_list_placeholder">
                                                <property name="selection-mode">none</property>
                                                <child>
                                                  <object class="AdwActionRow">
                                                    <property name="title" translatable="yes">No archives available</property>
                                                  </object>
                                                </child>
                                                <style>
                                                  <class name="boxed-list" />
                                                </style>
                                              </object>
                                            </child>
                                          </object>
                                        </child>
                                      </object>
                                    </child>
                                  </object>
                                </property>
                              </object>
                            </child>
                            <child>
                              <object class="AdwViewStackPage">
                                <property name="use-underline">1</property>
                                <property name="title" translatable="yes">_Schedule</property>
                                <property name="icon-name">schedule-symbolic</property>
                                <property name="child">
                                  <object class="AdwPreferencesPage" id="page_schedule">
                                    <child>
                                      <object class="AdwPreferencesGroup">
                                        <child>
                                          <object class="GtkListBox" id="schedule_status_list">
                                            <property name="selection-mode">none</property>
                                            <child>
                                              <object class="PikaStatusRow" id="schedule_status" />
                                            </child>
                                            <style>
                                              <class name="boxed-list" />
                                            </style>
                                          </object>
                                        </child>
                                      </object>
                                    </child>
                                    <child>
                                      <object class="AdwPreferencesGroup">
                                        <property name="title" translatable="yes">Scheduled Backups</property>
                                        <child>
                                          <object class="AdwExpanderRow" id="schedule_active">
                                            <property name="activatable">False</property>
                                            <property name="title" translatable="yes">Regularly Create Backups</property>
                                            <property name="expanded">True</property>
                                            <property name="show-enable-switch">True</property>
                                            <child>
                                              <object class="AdwComboRow" id="schedule_frequency">
                                                <property name="title" translatable="yes">Frequency</property>
                                                <property name="expression">
                                                  <lookup type="PikaBackupScheduleFrequency" name="display"></lookup>
                                                </property>
                                              </object>
                                            </child>
                                            <child>
                                              <object class="AdwActionRow" id="preferred_time_row">
                                                <property name="title" translatable="yes">Preferred Time</property>
                                                <property name="activatable-widget">schedule_preferred_time_button</property>
                                                <child>
                                                  <object class="GtkMenuButton" id="schedule_preferred_time_button">
                                                    <property name="focus-on-click">0</property>
                                                    <property name="halign">end</property>
